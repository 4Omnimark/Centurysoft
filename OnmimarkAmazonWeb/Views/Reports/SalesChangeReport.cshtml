@model IEnumerable<OmnimarkAmazon.Models.SalesChangeReportRecord>

<table>
    <tr>
        <th>
            ASIN
        </th>
        <th>
            Title
        </th>
        <th>
            30 Day
        </th>
        <th>
            7 Day
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ASIN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sales30Day)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sales7Day)
        </td>
    </tr>
}

</table>
